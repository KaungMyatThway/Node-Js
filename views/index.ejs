<!DOCTYPE html>
<html lang="en">
  <%- include('./components/head.ejs') %>
  <body>
    <%- include('./components/navbar.ejs') %>
    <% if (blogs.length) { %>
      <% blogs.forEach(blog => { %>
        <h1><%= blog.title %></h1>
        <p><%= blog.text %></p>
      <% }) %>
    <% } else { %>
      <h1>There is no blog</h1>
    <% } %>
  </body>
</html>
