<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <nav>
      <a href="/">HOME</a>
      <a href="/about">ABOUT</a>
    </nav>
    <% if (blogs.length) { %>
      <% blogs.forEach(blog => { %>
        <h1><%= blog.title %></h1>
        <p><%= blog.text %></p>
      <% }) %>
    <% } else { %>
      <h1>There is no blog</h1>
    <% } %>
  </body>
</html>
